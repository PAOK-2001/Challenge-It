<?xml version="1.0"?>
<robot name="Puzzlebot Jetson-Lidar Ed.">
   <material name="blue">
    <color rgba="0.68 0.83 0.9 0.8"/>
    </material>
    
   <material name="purple">
        <color rgba="0.54 0.12 0.33 1"/>
   </material>

  <link name="base_link">
  </link>
  
    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0.0 0.0 0.05"/>
    </joint>


     <joint name="left_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="left_wheel"/>
        <origin xyz="0 0 0"/>
    </joint>

    <joint name="right_wheel_joint" type="fixed">
        <parent link="chassis"/>
        <child link="right_wheel"/>
        <origin xyz="0 0 0"/>
    </joint>

    <!-- <joint name="camera_joint" type="fixed">
        <parent link="chassis"/>
        <child link="camera"/>
        <origin xyz="0 0 0"/>
    </joint> -->
    
    <link name="chassis">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.01" />
            <inertia ixx="7.58333333333e-06" ixy="0.0" ixz="0.0" iyy="7.58333333333e-06" iyz="0.0" izz="1.25e-05"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.57079632679 0 1.57079632679" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
            <material name="blue"/>
        </visual>
    </link>

    <link name="left_wheel">
        <collision>
            <origin rpy="0 0 0" xyz="0.05 0.095 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_1_1_Wheel_Coupler_2.stl"/>
            </geometry>
        </collision>

         <visual>
            <origin rpy="1.57079632679 0 0" xyz="0.05 0.095 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_1_1_Wheel_Coupler_2.stl"/>
            </geometry>
            <material name="purple"/>
        </visual>
    </link>


    <link name="right_wheel">
        <collision>
            <origin rpy="0 0 0" xyz="0.05 -0.095 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_1_1_Wheel_Coupler_2.stl"/>
            </geometry>
        </collision>

         <visual>
            <origin rpy="-1.57079632679 0 0" xyz="0.05 -0.095 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_1_1_Wheel_Coupler_2.stl"/>
            </geometry>
            <material name="purple"/>
        </visual>
    </link>

    <!-- <link name="camera">
        <visual>
            <origin rpy="-1.57079632679 0 0" xyz="0.05 -0.095 0"/>
            <geometry>
                <mesh filename="package://localisation/models/camera.stl"/>
            </geometry>
            <material name="purple"/>
        </visual>
    </link> -->

</robot>
